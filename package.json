{
  "name": "vibecodemanager-monorepo",
  "version": "1.2.0",
  "private": true,
  "description": "VibeCodeManager monorepo - AI-powered dictation with MCP integration",
  "scripts": {
    "dev": "pnpm --filter @vibecodemanager/desktop dev",
    "dev:mobile": "pnpm --filter @vibecodemanager/mobile start",
    "build": "pnpm --filter @vibecodemanager/shared build && pnpm --filter @vibecodemanager/desktop build",
    "build:shared": "pnpm --filter @vibecodemanager/shared build",
    "test": "pnpm -r test",
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format"
  },
  "packageManager": "pnpm@9.12.1",
  "pnpm": {
    "overrides": {
      "dotenv-expand": "11.0.6"
    }
  }
}

